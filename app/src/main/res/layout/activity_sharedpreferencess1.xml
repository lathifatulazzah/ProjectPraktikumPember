<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".sharedpreferencess1">

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="280dp"
        android:layout_height="238dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo2" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/nama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="276dp"
        android:text="Nama :"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.224"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/nama2"
        android:layout_width="134dp"
        android:layout_height="29dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="276dp"
        android:text="..."
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/nama"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/paket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Paket :"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/nama"
        app:layout_constraintTop_toBottomOf="@+id/nama" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/paket2"
        android:layout_width="133dp"
        android:layout_height="22dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="32dp"
        android:text="..."
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/paket"
        app:layout_constraintTop_toBottomOf="@+id/nama2" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/berat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Berat :"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/paket"
        app:layout_constraintTop_toBottomOf="@+id/paket" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/berat2"
        android:layout_width="133dp"
        android:layout_height="26dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="32dp"
        android:text="..."
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/berat"
        app:layout_constraintTop_toBottomOf="@+id/paket2" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/harga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Harga :"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/berat"
        app:layout_constraintTop_toBottomOf="@+id/berat" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/harga2"
        android:layout_width="128dp"
        android:layout_height="30dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="32dp"
        android:text="..."
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/harga"
        app:layout_constraintTop_toBottomOf="@+id/berat2" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/totalharga"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Total harga :"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/harga"
        app:layout_constraintTop_toBottomOf="@+id/harga" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/totalharga2"
        android:layout_width="90dp"
        android:layout_height="29dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:text="............."
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/totalharga"
        app:layout_constraintTop_toBottomOf="@+id/harga2" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/pensil"
        android:layout_width="57dp"
        android:layout_height="54dp"
        android:layout_gravity="bottom|end"
        android:layout_marginStart="327dp"
        android:layout_marginEnd="28dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.865"
        app:srcCompat="@drawable/edit" />

    <Button
        android:id="@+id/btnkembali"
        android:layout_width="146dp"
        android:layout_height="61dp"
        android:layout_marginTop="348dp"
        android:text="kembali"
        app:layout_constraintEnd_toEndOf="@+id/imageView8"
        app:layout_constraintHorizontal_bias="0.544"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/imageView8" />

</androidx.constraintlayout.widget.ConstraintLayout>